{
  "name": "react-podcast-app",
  "description": "A small React app for iTunes podcasts",
  "version": "1.0.0",
  "author": {
    "name": "Sema GarcÃ­a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semagarcia/react-podcast-app.git"
  },
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/semagarcia/react-podcast-app/issues"
  },
  "homepage": "https://github.com/semagarcia/react-podcast-app#readme",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  },
  "scripts": {
    "start": "webpack serve --progress --config webpack.dev.js --open",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "bundle-analyzer:open-report:dev": "open ./reports/bundle-info/dev-bundle-report.html",
    "bundle-analyzer:open-report:prod": "webpack-bundle-analyzer ./reports/bundle-info/prod-bundle-report.json",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write --list-different --ignore-path .eslintignore \"./**/*.js\"",
    "format:stylelint": "stylelint --fix --allow-empty-input --ignore-path .eslintignore \"./src/*.scss\"",
    "format:package": "sort-package-json package.json",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ignore-path .eslintignore --ext .js .",
    "lint:stylelint": "stylelint --allow-empty-input --ignore-path .eslintignore \"./**/*.scss\"",
    "lint:prettier": "prettier --check --ignore-path .eslintignore \"./**/*.js\"",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest --config ./jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-scss-transform": "^1.0.3",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "sort-package-json": "^2.1.0",
    "storybook": "^6.5.14",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
